rm(list = ls()) #clear work space

?dplyr
listings = as_tibble(read.csv("querydata_r.csv", stringsAsFactors = F))
str(listings)

###Arts and Humanities

#USA
all_USA = listings[listings$country1 == "United States"|listings$country2 == "United States"|listings$country3 == "United States" |listings$country4 == "United States" |listings$country5 == "United States" 
                |listings$country6 == "United States" |listings$country7 == "United States" |listings$country8 == "United States" |listings$country9 == "United States",] 

Arts_USA = all_USA[all_USA$ASJC.B2 == "Arts and Humanities"| all_USA$ASJC.C2 == "Arts and Humanities"| all_USA$ASJC.D2 == "Arts and Humanities" | all_USA$ASJC.E2 == "Arts and Humanities" 
                   | all_USA$ASJC.F2 == "Arts and Humanities" | all_USA$ASJC.G2 == "Arts and Humanities" | all_USA$ASJC.H2 == "Arts and Humanities" | all_USA$ASJC.I2 == "Arts and Humanities" 
                   | all_USA$ASJC.J2 == "Arts and Humanities"| all_USA$ASJC.K2 == "Arts and Humanities" | all_USA$ASJC.L2 == "Arts and Humanities" | all_USA$ASJC.M2 == "Arts and Humanities"
                   | all_USA$ASJC.N2 == "Arts and Humanities",]

write.csv(Arts_USA, file = "Arts_USA.csv",)


